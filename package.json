{
  "name": "@goa/compress",
  "version": "0.0.0-pre",
  "description": "Compression Middleware For Goa Apps.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "lib": "depack src/depack.js -o compile/compress.js -a -c --source_map_include_content",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "doc -o README.md -n _compress -a -d",
    "wiki": "doc wiki -W wiki.git -n _compress -a -d",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u -t types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js"
  },
  "files": [
    "src",
    "compile",
    "types/externs.js",
    "typedefs.json"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/compress.git"
  },
  "keywords": [
    "idio",
    "compress",
    "goa",
    "middleware",
    "gzip"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/idiocc/compress/issues"
  },
  "homepage": "https://www.idio.cc",
  "devDependencies": {
    "alamode": "^3.2.0",
    "depack": "^1.0.1",
    "documentary": "^1.34.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  }
}